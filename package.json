{
	"name": "client",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@types/jest": "24.0.11",
		"@types/node": "11.11.6",
		"@types/react": "16.8.8",
		"@types/react-dom": "16.8.3",
		"husky": "^1.3.1",
		"lint-staged": "^8.1.5",
		"prettier": "^1.17.0",
		"react": "^16.8.5",
		"react-dom": "^16.8.5",
		"react-scripts": "^2.1.8",
		"typescript": "3.3.4000"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"storybook": "start-storybook -p 9009 -s public",
		"launch-tests": "sh launch-tests.sh",
		"build-storybook": "build-storybook -s public"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
			"prettier --single-quote --write",
			"git add"
		]
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"devDependencies": {
		"@storybook/react": "^5.0.6",
		"@storybook/addon-actions": "^5.0.6",
		"@storybook/addon-links": "^5.0.6",
		"@storybook/addons": "^5.0.6",
		"@babel/core": "^7.4.3",
		"babel-loader": "^8.0.5"
	}
